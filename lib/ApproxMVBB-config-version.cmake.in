set( PACKAGE_VERSION "@ApproxMVBB_VERSION@" )
if( "${PACKAGE_FIND_VERSION}" VERSION_EQUAL "@ApproxMVBB_VERSION@")
    set(PACKAGE_VERSION_EXACT 1)
endif()
if( "${PACKAGE_FIND_VERSION_MAJOR}.${PACKAGE_FIND_VERSION_MINOR}" EQUAL "@ApproxMVBB_VERSION@" )
    set(PACKAGE_VERSION_COMPATIBLE 1)
elseif( "${PACKAGE_FIND_VERSION_MAJOR}" EQUAL "@ApproxMVBB_VERSION_MAJOR@" )
    # for now backward compatible if minor version is less
    if( ${PACKAGE_FIND_VERSION_MINOR} LESS @ApproxMVBB_VERSION_MINOR@ )
        set(PACKAGE_VERSION_COMPATIBLE 1)
    endif()
endif()